{
	"datasetPath": "../datasets/",
	"output": "../output/",
	"seed": 72,
	// 维度
	"rel_dim": 300,  // 300
	"beta1": 1.0,
	"beta2": 1.0,
	"gamma_rel": 10.0,


	// 训练
	"optim_type": "Adagrad",  //1000,500
	"metric": "L1",	 // L1  L2  cosine
  	"train_epochs": 2000,  //1000,500

	//验证
	"early_stop": true,
	"start_valid": 50,  //
	"eval_freq": 10,   //
	"eval_save_freq": 10,  //

	"patience": 10,
	"patience_minloss": 20,

	//采样
	"neg_k": 50,

	//超参
	"learning_rate": 0.001,  //0.01, 0.005, 0.001
	"weight_decay": 5e-4,
	"LeakyReLU_alpha": 0.2,
	"dropout": 0,   // 0.1, 0.2, 0.3
	"top_k": [1, 3, 5, 10, 50, 100]
}

